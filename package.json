{
  "name": "site",
  "version": "0.1.0",
  "sideEffects": false,
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "next dev",
    "lint-staged:test": "lint-staged",
    "build:postcss": "postcss styles/output.css -u autoprefixer cssnano -r --no-map",
    "unused": "npx depcheck",
    "watch:tailwind": "npx tailwindcss -i styles/global.css -o styles/output.css --watch"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@algolia/autocomplete-core": "^1.7.2",
    "@algolia/autocomplete-js": "^1.7.2",
    "@chec/commerce.js": "^2.8.0",
    "@emailjs/browser": "^3.7.0",
    "@stripe/react-stripe-js": "^1.12.0",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "0.5.3",
    "@types/chec__commerce.js": "^2.8.5",
    "@types/stripe": "^8.0.417",
    "algoliasearch": "^4.14.2",
    "cssnano": "^5.1.13",
    "csstype": "^3.1.1",
    "depcheck": "^1.4.3",
    "lint-staged": "^13.0.3",
    "next": "12.1.6",
    "next-auth": "4.10.3",
    "next-themes": "^0.2.0",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hot-toast": "^2.4.0",
    "react-instantsearch-hooks-web": "^6.33.0",
    "stripe": "^10.13.0",
    "tailwindcss": "3.1.5"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/react": "18.0.8",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.6",
    "typescript": "4.6.4"
  }
}
