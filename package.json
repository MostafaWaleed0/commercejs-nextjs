{
  "name": "site",
  "version": "0.1.0",
  "sideEffects": false,
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "next dev",
    "lint-staged:test": "lint-staged",
    "build:postcss": "postcss styles/output.css -u autoprefixer cssnano -r --no-map",
    "unused": "npx depcheck",
    "watch:tailwind": "npx tailwindcss -i styles/global.css -o styles/output.css --watch"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@algolia/autocomplete-core": "^1.7.3",
    "@algolia/autocomplete-js": "^1.7.3",
    "@chec/commerce.js": "^2.8.0",
    "@emailjs/browser": "^3.9.1",
    "@stripe/react-stripe-js": "^1.14.2",
    "@stripe/stripe-js": "^1.44.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "0.5.8",
    "@types/chec__commerce.js": "^2.8.5",
    "@types/stripe": "^8.0.417",
    "algoliasearch": "^4.14.2",
    "cssnano": "^5.1.14",
    "csstype": "^3.1.1",
    "depcheck": "^1.4.3",
    "lint-staged": "^13.0.3",
    "next": "^13.0.3",
    "next-auth": "4.16.4",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-instantsearch-hooks-web": "^6.38.0",
    "stripe": "^10.17.0",
    "tailwindcss": "3.2.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "18.0.25",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "typescript": "4.8.4"
  }
}
